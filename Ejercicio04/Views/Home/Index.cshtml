@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Tablas de multiplicar</h1>
    <table class="table table-bordered">
        @for (int i = 1; i <= 10; i++)
        {
            <tr>
                @for (int j = 1; j <= 10; j++)
                {
                    <td>
                         @i * @j = @(i*j)
                    </td>
                }
            </tr>
        }
    </table>
</div>
